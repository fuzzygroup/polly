<div id="<%= dom_id musician %>">
  <p>
    <strong>Name:</strong>
    <%= musician.name %>
  </p>

  <p>
    <strong>Band name:</strong>
    <%= musician.band_name %>
  </p>

  <p>
    <strong>Bio:</strong>
    <%= musician.bio %>
  </p>

  <p>
    <strong>Url:</strong>
    <%= musician.url %>
  </p>

  <p>
    <strong>Pronouns:</strong>
    <%= musician.pronouns %>
  </p>

  <p>
    <strong>Play count:</strong>
    <%= musician.play_count %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= musician.notes %>
  </p>

  <p>
    <strong>User:</strong>
    <%= musician.user_id %>
  </p>

  <p>
    <strong>Contact card:</strong>
    <%= musician.contact_card_id %>
  </p>

</div>
