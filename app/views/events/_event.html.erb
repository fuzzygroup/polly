<div id="<%= dom_id event %>">
  <p>
    <strong>Name:</strong>
    <%= event.name %>
  </p>

  <p>
    <strong>Event type:</strong>
    <%= event.event_type_id %>
  </p>

  <p>
    <strong>Date start:</strong>
    <%= event.date_start %>
  </p>

  <p>
    <strong>Date end:</strong>
    <%= event.date_end %>
  </p>

  <p>
    <strong>Time start:</strong>
    <%= event.time_start %>
  </p>

  <p>
    <strong>Time end:</strong>
    <%= event.time_end %>
  </p>

  <p>
    <strong>Location:</strong>
    <%= event.location %>
  </p>

  <p>
    <strong>Share code:</strong>
    <%= event.share_code_id %>
  </p>

  <p>
    <strong>Organization:</strong>
    <%= event.organization_id %>
  </p>

  <p>
    <strong>User:</strong>
    <%= event.user_id %>
  </p>

  <p>
    <strong>Has speakers:</strong>
    <%= event.has_speakers %>
  </p>

  <p>
    <strong>Has musicians:</strong>
    <%= event.has_musicians %>
  </p>

  <p>
    <strong>Has props:</strong>
    <%= event.has_props %>
  </p>

  <p>
    <strong>Has partners:</strong>
    <%= event.has_partners %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= event.body %>
  </p>

</div>
