

    <tr>
      <% if Rails.env.development? %>
        <td scope="row">
          <%= team.id%>
        </td>
      <% end %>
      <td><%= link_to(team.name, team_path(team)) %></td>
      <td><%= link_to(team.user.full_name, user_path(team.user)) %></td>
      <td class="text-center"><%= show_active(team) %></td>
      <td class="text-center"><%=team.team_users.count%></td>
      <td class="text-center"><%=team.team_rules.count%></td>
      <td class="text-center"><%= chat_link(team)%></td>
    </tr>
